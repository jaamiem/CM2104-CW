<div id="resultsPane">
	<a id="closeSearch" class="btn accBtn" href="#">close</a>
	<h3>Search</h3>
	<p>for "<%= query %>"</p>
	<div class="list-group">
		<% spots.forEach(function(item) { %>
			<a href="#" class="list-group-item">
				<%= item.name %>
				<br>
				<%= item.type %>
			</a>
		<% }); %>
	</div>
</div>
